  .hero-content {
    position: relative;
    z-index: 10;
  }

  /* Section separators (slanted like reference) */
  .slant-top { position: relative; }
  .slant-top::before {
    content:"";
    position:absolute;
    left:0;
    right:0;
    top:-80px;
    height:80px;
    background: var(--dg-900);
    clip-path: polygon(0 100%,100% 0,100% 100%);
  }

  /* Green tint overlay for photos */
  .green-tint { position: relative; }
  .green-tint::after {
    content:"";
    position:absolute;
    inset:0;
    background: rgba(31, 91, 69, 0.55);
    border-radius: inherit;
  }

  /* Buttons */
  .btn-primary {
    background: var(--dg-300);
    color:#fff;
    font-weight:700;
    border-radius:.5rem;
    padding:.75rem 1.25rem;
    display:inline-block;
    transition: transform .15s ease, filter .15s ease, background .15s ease;
  }
  .btn-primary:hover {
    background: var(--dg-400);
    filter:brightness(1.02);
    transform: translateY(-1px);
  }

  .tick-item::before {
    content:"✔";
    margin-right:.5rem;
    font-weight:700;
    color: var(--dg-200);
  }

  /* Mobile overrides for hero */
