<style>
  :root {
    --dg-900:#0b1f19;
    --dg-800:#0f2a22;
    --dg-700:#14362b;
    --dg-600:#184233;
    --dg-500:#1c4e3c; /* primary */
    --dg-400:#1f5b45;
    --dg-300:#2a7a5a;
    --dg-200:#9ed1b7;
    --dg-100:#e6f3ed;
  }

  /* NAV: dropdown */
  .dropdown-menu { display: none; }
  .dropdown:hover .dropdown-menu,
  .dropdown:focus-within .dropdown-menu,
  .dropdown-menu:hover { display: block; }

  /* HERO (triangular) */
  .hero {
    position: relative;
    background: url('/javidbest.png') no-repeat center center/cover;
    clip-path: polygon(0 0, 100% 0, 100% 82%, 0 100%);
    height: 90vh;
  }
  .hero::after {
    content: "";
    position: absolute;
    inset: 0;
    background: rgba(12, 34, 27, 0.78); /* dark green overlay */
  }
  .hero-content { position: relative; z-index: 10; }

  /* Section separators (slanted like reference) */
  .slant-top { position: relative; }
  .slant-top::before {
    content:"";
    position:absolute;
    left:0; right:0; top:-80px; height:80px;
    background: var(--dg-900);
    clip-path: polygon(0 100%,100% 0,100% 100%);
  }

  /* Green tint overlay for photos (kept for parity) */
  .green-tint { position: relative; }
  .green-tint::after {
    content:"";
    position:absolute; inset:0;
    background: rgba(31, 91, 69, 0.55);
    border-radius: inherit;
  }

  /* Buttons */
  .btn-primary {
    background: var(--dg-300);
    color:#fff; font-weight:700;
    border-radius:.5rem;
    padding:.75rem 1.25rem;
    display:inline-block;
    transition: transform .15s ease, filter .15s ease, background .15s ease;
  }
  .btn-primary:hover {
    background: var(--dg-400);
    filter:brightness(1.02);
    transform: translateY(-1px);
  }

  .tick-item::before {
    content:"✔";
    margin-right:.5rem;
    font-weight:700;
    color: var(--dg-200);
  }

  /* Mobile overrides for hero */
  @media (max-width: 768px) {
    .hero {
      background: url('/javidbest-mobile.png') no-repeat center center/cover;
      height: 80vh;
    }
  }
</style>

