      <div class="flex gap-3 flex-wrap justify-center md:justify-end">
        <button id="reject-cookies"
                class="bg-gray-200 text-black px-5 py-1.5 rounded-md hover:bg-gray-300 text-sm font-medium">
          Förneka
        </button>
        <button id="accept-cookies"
                class="bg-green-500 hover:bg-green-600 text-white font-semibold px-5 py-1.5 rounded-md text-sm">
          Acceptera
        </button>
      </div>
    </div>
  </div>

  <script>
    document.addEventListener('DOMContentLoaded', function () {
      const banner = document.getElementById("cookie-banner");
      const overlay = document.getElementById("overlay");
      const acceptBtn = document.getElementById("accept-cookies");
      const rejectBtn = document.getElementById("reject-cookies");

      function loadGA() {
        const gaScript = document.createElement('script');
        gaScript.src = 'https://www.googletagmanager.com/gtag/js?id=G-7G28B2HMYY';
        gaScript.async = true;
        document.head.appendChild(gaScript);

        gaScript.onload = () => {
          window.dataLayer = window.dataLayer || [];
          function gtag() { dataLayer.push(arguments); }
          gtag('js', new Date());
          gtag('config', 'G-7G28B2HMYY');
        };
      }

      function hideBanner() {
        banner.style.display = 'none';
        overlay.style.display = 'none';
      }

      if (acceptBtn && rejectBtn) {
        acceptBtn.addEventListener('click', () => {
          localStorage.setItem('cookieConsent', 'accepted');
          loadGA();
          hideBanner();
        });

        rejectBtn.addEventListener('click', () => {
          localStorage.setItem('cookieConsent', 'rejected');
          hideBanner();
        });
      }

      const consent = localStorage.getItem('cookieConsent');
      if (consent === 'accepted') {
        loadGA();
      } else if (!consent) {
        setTimeout(() => {
          banner.style.display = 'block';
          overlay.style.display = 'block';
        }, 3000);
      }
    });
  </script>

</body>
